{
  "name": "jpl-horizons-client",
  "version": "0.1.0",
  "description": "Fetch state vectors for Solar System bodies from the JPL HORIZONS on-line solar system data and ephemeris.",
  "main": "./dist/index.js",
  "bin": {
    "fetch-bodies": "bin/fetch-bodies"
  },
  "scripts": {
    "test": "jest",
    "start": "nodemon",
    "prettier": "prettier --single-quote --print-width 80 \"./**/*.{js,ts}\" --write",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "./**/*.{js,ts}": [
        "prettier --single-quote --print-width 80 \"./**/*.{js,ts}\" --write",
        "jest --bail --coverage --findRelatedTests"
      ]
    }
  },
  "author": "Darrell A. Huffman",
  "license": "GNU General Public License v3.0",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "cross-fetch": "^3.0.2",
    "yargs": "^13.2.4"
  }
}
